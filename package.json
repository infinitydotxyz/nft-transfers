{
  "name": "nft-transfers",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "prepare": "husky install",
    "start": "node ./dist/index.js",
    "start:prod": "npm run build && node ./dist/index.js",
    "start:dev": "cross-env NODE_PATH=./src ts-node-dev ./src/index.ts",
    "update:lib": "npm update @infinity/lib"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@infinityxyz/lib": "^1.38.17",
    "chalk": "^5.0.1",
    "emittery": "^0.10.2",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2"
  }
}
